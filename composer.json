{
  "name": "hweihwang/laravel-queue-kafka",
  "description": "Kafka driver for Laravel Queue",
  "license": "MIT",
  "type": "library",
  "version": "1.0",
  "authors": [
    {
      "name": "Peter Mein",
      "email": "petermein@gmail.com"
    },
    {
      "name": "Hoang Pham",
